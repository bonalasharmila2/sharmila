#include <stdio.h>
#include <string.h>
struct Person {
  char name[10];
  int citNo;
  float salary;
} person1;
int main() {
  strcpy(person1.name, "Bell vein");
  person1.citNo = 202;
  person1. salary = 20000;
  printf("Name: %s\n", person1.name);
  printf("Citizenship No.: %d\n", person1.citNo);
  printf("Salary: %.2f", person1.salary);
  return 0;
}
output:
Name: Bell vein
citizen ship no:202
salary:20000
